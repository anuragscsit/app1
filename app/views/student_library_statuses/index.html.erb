<div class="container">
	<div class="col-lg-12">
		<div class="col-lg-6">
			<div class="col-sm-12">
				<div class="col-sm-3">
					<b>Issue Date:</b>
				</div>
				<div class="col-sm-3">
					<b>Submit Date</b>
				</div>
				<div class="col-sm-3">
					<b>Submit On</b>
				</div>
				<div class="col-sm-3">
					<b>Student</b>
				</div>
			</div>
			<div class="col-sm-12">
				<% @student_library_statuses.each do |sls| %>
				<div class="col-sm-3">
					<%= sls.issue_date %>
				</div>
				<div class="col-sm-3">
					<%= sls.submission_date %>
				</div>
				<div class="col-sm-3">
					<%= sls.submitted_on %>
				</div>
				<div class="col-sm-3">
					<%= sls.book_name %>
				</div>
				<% end %>
			</div>
		</div>
	</div>
	<%= link_to 'New Status', new_student_library_status_path, class: 'btn btn-primary' %>
</div>