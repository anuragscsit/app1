<div class="container">
	<div class="col-lg-12">
		<div class="col-lg-2">
			Name: 
		</div>
		<div class="col-lg-2">
			Enrollment Number:
		</div>
		<div class="col-lg-2">
			Member Id: 
		</div>
		<div class="col-lg-2">
			Contact:
		</div>
		<div class="col-lg-2">
			Address:
		</div>
		<div class="col-lg-2">
			Section
		</div>
	</div>
	<div class="col-lg-12">
		<% @students.each do |s| %>
			<div class="col-lg-2">
				<%= s.name%>
			</div>
			<div class="col-lg-2">
				<%= s.enrollment_no %>
			</div>
			<div class="col-lg-2">
				<%= s.member_id %>
			</div>
			<div class="col-lg-2">
				<%= s.contact %>
			</div>
			<div class="col-lg-2">
				<%= s.adderss%>
			</div>
			<div class="col-lg-2">
				<%= s.name%>
			</div>
		<% end %>
	</div>
	<%= link_to 'New Student', new_student_path , class: 'btn btn-primary' %>
</div>